#include <iostream>
#include <math.h>
using namespace std;

int main()
{
setlocale(LC_ALL,"RUS");
int n,i,a,b;

cout<<"Введите число N>2: ";
cin>>n;

cout<<"Введите число A: ";
cin>>a;

cout<<"Введите число B: ";
cin>>b;

int arr[n];
arr[0]=a;
arr[1]=b;

for (i=0;i<(n-2);i++)arr[i+2]=arr[i]+arr[i+1];
for (i=0;i<n;i++)cout<<arr[i]<<" ";
}
